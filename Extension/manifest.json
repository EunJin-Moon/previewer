{
    "name": "catch-data-test",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "ChromeTest explain",
    "background": {
        "scripts": [
          "jQuery/jquery-3.4.1.js",
          "background.js"
        ],
        "pages": [
          "popup.html"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "default",
        "default_icon": "Icons/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "http://localhost/"
    ]
}
