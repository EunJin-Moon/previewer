{
    "name": "catch-data-test",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "ChromeTest explain",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "default",
        "default_icon": "icon.png"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}