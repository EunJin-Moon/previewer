{
    "name": "catch-data-test",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "ChromeTest explain",
    "background": {
        "scripts": [
          "jQuery/jquery-3.4.1.js",
          "background.js"
        ],
        "pages": [
          "popup.html"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "default",
        "default_icon": "Icons/icon.png"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "http://localhost/"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
