{
    "name": "Previewer",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Detect threat",
    "background": {
        "scripts": [
          "Vue.js",
          "background.js"
        ],
        "pages": [
          "status.html"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "default",
        "default_icon": "Icons/icon.png",
        "default_popup": "status.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "http://localhost/",
        "https://cdn.jsdelivr.net/npm/vue@2.x/dist/vue.js",
        "https://cdn.jsdelivr.net/npm/vuetify@2.x/dist/vuetify.js"
    ]
}
